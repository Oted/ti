        axsIdParams.axsIdFacebookAppId = "417096598310966";
        axsIdParams.axsIdContactSupport = "https://support.axs.com";
        axsIdParams.axsIdPrivacyPolicy = "http://www.axs.com/about-privacy-policy.html";
        axsIdParams.axsIdTerms = "http://www.axs.com/about-terms-of-use.html";
        axsIdParams.country = "US";
        axsIdParams.axsSDKMobile = "";
        axsIdParams.triggerFlashMigrationFlow = "1";
        axsIdParams.autoCreateFlashAccount = "1";
        axsIdParams.load = "signin";
        axsIdParams.axsIdSkin = "";
        axsIdParams.callbackUrl = window.location.href;
        axsIdParams.detectLocale = true;
        axsIdParams.mpsRegion = "1";
        axsIdParams.enableMarketingAgreement = "";
        axsIdParams.loadSubOptions = "blizzard";
        axsIdParams.callback = function (err, user) {
            var callbackUrl = axsIdParams.callbackUrl;
            digitalData.userData = {
                profile: {
                    profileId: user.email,
                    loginType: user.type
                }
            }
            if(_satellite){
                _satellite.track('login');
            }

            if (callbackUrl) {
                axs.Core.loadingScreen.show();
                var CTAPageRegex = [/\/events\//];
                var redirectToMyEvents = true;
                CTAPageRegex.forEach(function(ele){
                    if(callbackUrl.match(ele)){
                        redirectToMyEvents = false;
                    }
                });
                if(redirectToMyEvents){
                    callbackUrl = "https://www.axs.com/me/orders";
                }
                window.location.href = callbackUrl;
            }
        };
        axsIdParams.onInitCallback = function () {
            $('#axsid_login_loader').remove();
        };

        axs.AXSID.startAXSId(axsIdParams);
</script>

