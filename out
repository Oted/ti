<script type="text/javascript">
    // pageKey is needed for logging a string against the routing of a page
    // Example: Trending Topics

    var axs = window.axs || {};
    axs.Core.pageKey = (function () {
        var route = $.trim('venues_details_base');

        switch (route) {
            case 'default_index_base':
            case 'default_index':
                return 'H'
                break;
            case 'article_details_base':
            case 'article_details':
                return 'A'
                break;
        }
    })();
