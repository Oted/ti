            {{#if combineDatesArray.length }}
                <div class="date-block date-block--search date-block--multiple-dates">
            {{else}}
                <div class="date-block date-block--search">
            {{/if }}

                    {{#ifvalue eventDate equals="TBD"}}
                        <div class="date-block--tbd"> {{translate "translations.tbd" }} </div>
                    {{else}}
                        {{#if combineDatesArray.length  }}
                            <div class="date-block__starting"> {{translate "translations.starting" }} </div>
                            <div class="date-block__day"> {{formatDate eventDate "D"}} </div>
                            <div class="date-block__month"> {{formatDate eventDate "MMM"}} </div>
                        {{else}}
                            <div class="date-block__month"> {{formatDate eventDate "MMM"}} </div>
                            <div class="date-block__day"> {{formatDate eventDate "D"}} </div>
                            <div class="date-block__week-day"> {{formatDate eventDate "ddd"}} </div>
                        {{/if}}

                    {{/ifvalue}}
                </div>

            <img src="{{ eventImage }}" class="event--img">

        </div>
        <div class="results-table__col--event">
